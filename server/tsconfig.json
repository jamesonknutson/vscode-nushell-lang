{
	"compilerOptions": {
		"target": "ESNext",
		"lib": [
			"ESNext"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"strictNullChecks": true,
		"sourceMap": true,
		"strict": true,
		"outDir": "out",
		"esModuleInterop": true,
		"composite": true,
		"rootDir": "src"
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}