{
	"editor.insertSpaces": false,
	"typescript.tsc.autoDetect": "off",
	"typescript.preferences.quoteStyle": "single",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"github.copilot.inlineSuggest.enable": false,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/*.jpg": true,
		"**/*.mp4": true,
		"**/.classpath": true,
		"**/.factorypath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/*.git": true,
		".github": true,
		".vscode": true,
		"assets": true,
		"scripts": true,
		"snippets": true,
		"src": true,
		"tests": true,
		".gitattributes": true,
		".gitignore": true,
		"used_keywords.jsonc": true,
		"nushell.theme-minimal-light.json": true,
		"nushell.theme-minimal-dark.json": true,
		"LICENSE": true,
		"example.nu": true,
		"generate-example.nu": true,
		"generate-patterns.nu": true,
		".prettierignore": true,
		".eslintignore": true,
		".prettierrc": true,
		".vscodeignore": true,
		"package-lock.json": true,
		"client/node_modules": true,
		"client/package-lock.json": true,
		"server/package-lock.json": true,
		"client/src/test": true
	},
	"explorerExclude.backup": {},
	"hide-files.files": [
		".github",
		".vscode",
		"assets",
		"scripts",
		"snippets",
		"src",
		"tests",
		".gitattributes",
		".gitignore",
		"used_keywords.jsonc",
		"nushell.theme-minimal-light.json",
		"nushell.theme-minimal-dark.json",
		"LICENSE",
		"example.nu",
		"generate-example.nu",
		"generate-patterns.nu",
		".prettierignore",
		".eslintignore",
		".prettierrc",
		".vscodeignore",
		"package-lock.json",
		"client/node_modules",
		"client/package-lock.json",
		"server/package-lock.json",
		"client/src/test"
	]
}